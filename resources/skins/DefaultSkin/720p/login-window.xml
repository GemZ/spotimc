<window id="4002" type="dialog">
    <defaultcontrol always="false">1000</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <backgroundcolor>0000000</backgroundcolor>
    <!-- <views>50,51,509,510<views> -->
    <!-- <animation effect="fade" time="100">WindowOpen</animation> -->
    <!-- <animation effect="slide" end="0,576" time="100">WindowClose</animation> -->
    
    
    <!-- Open animation -->
    <!--
    <animation type="WindowOpen">
    	<effect type="slide" center="640,360" time="150" start="0,-360" end="0,0" />
    </animation>
    
    <animation effect="fade" time="1000">WindowClose</animation>
    -->
    
    <visible>true</visible>
    <zorder>1</zorder>
    <coordinates>
	    <posx></posx>
	    <posy></posy>
    </coordinates>
    <previouswindow></previouswindow>
    <useincludes>false</useincludes>
    
    <controls>
    	<!-- Window background -->
    	<!--
    	<control type="image">
            <description>Background overlay</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <visible>true</visible>
            <colordiffuse>80ffffff</colordiffuse>
            <fadetime></fadetime>
            <texture>common/overlay.png</texture>
            <aspectratio>stretch</aspectratio>
        </control>
        -->
        
        <control type="group" id="1000">
            <description>Login container</description>
            <defaultcontrol>1100</defaultcontrol>
            <visible>true</visible>
            <posx>384</posx>
            <posy>160</posy>
            <width>480</width>
            <height>384</height>
            
            <animation type="WindowOpen">
		    	<effect type="slide" tween="cubic" easing="out" center="640,360" time="200" start="0,-600" end="0,0" />
		    </animation>
		    
		    <animation type="Hidden">
		    	<effect type="slide" tween="cubic" easing="in" center="640,360" time="200" start="0,0" end="0,600" />
		    </animation>
		    
		    
		    <control type="image">
		    	<include>spotymc_dialog_border</include>
	            <description>Window border</description>
	            <posx>-16</posx>
	            <posy>-16</posy>
	            <width>512</width>
	            <height>416</height>
	        </control>
	        
	        <control type="image">
	            <description>Window background image</description>
	            <posx>0</posx>
	            <posy>0</posy>
	            <width>480</width>
	            <height>384</height>
	            <visible>true</visible>
	            <colordiffuse>ffffffff</colordiffuse>
	            <fadetime></fadetime>
	            <texture>common/login-bg.png</texture>
	            <aspectratio>stretch</aspectratio>
	        </control>
            
            <!-- Login prompt text -->
            <control type="label">
            	<label>Login to Spotify</label>
				<posx>32</posx>
				<posy>32</posy>
				<width>416</width>
				<height>32</height>
				<font>spotymc_font24</font>
				<align>center</align>
				<aligny>center</aligny>
				<selectedcolor></selectedcolor>
				<textcolor>ffffffff</textcolor>
			</control>
			
			
			<control type="group" id="1100">
				<description>Login fields container</description>
	            <defaultcontrol>1101</defaultcontrol>
	            <visible>true</visible>
	            <posx>32</posx>
	            <posy>80</posy>
	            <width>480</width>
	            <height>384</height>
			
				<control type="label">
					<include>spotymc_login_inputlabel</include>
	                <label>Username</label>
	                <posx>0</posx>
	                <posy>0</posy>
	            </control>
	            
				<control type="button" id="1101">
					<include>spotymc_login_textinput</include>
					<description>Username input</description>
					<posx>0</posx>
					<posy>32</posy>
					<onclick></onclick>
					<onfocus></onfocus>
					<onup></onup>
					<ondown>1102</ondown>
					<onleft></onleft>
					<onright></onright>
	            </control>
	            
	            
	            <control type="label">
	            	<include>spotymc_login_inputlabel</include>
	                <label>Password</label>
	                <posx>0</posx>
	                <posy>80</posy>
	            </control>
	            
	            <control type="button" id="1102">
	            	<include>spotymc_login_textinput</include>
	                <description>Password input</description>
	                <posx>0</posx>
	                <posy>112</posy>
	                <label></label>
	                <onclick></onclick>
	                <onfocus></onfocus>
	                <onup>1101</onup>
	                <ondown>1103</ondown>
	                <onleft></onleft>
	                <onright></onright>
	            </control>
	            
	            <control type="label">
	            	<include>spotymc_login_inputlabel</include>
	                <label>Remember me</label>
	                <posx>40</posx>
	                <posy>168</posy>
	            </control>
	            
	            <control type="togglebutton" id="1103">
	            	<include>spotymc_login_toggle</include>
		            <description>Remember me toggle</description>
		            <posx>0</posx>
		            <posy>168</posy>
		            <onfocus></onfocus>
		            <onup>1102</onup>
		            <ondown>1104</ondown>
		            <onleft></onleft>
		            <onright></onright>
		            <label></label>
		            <usealttexture>Skin.HasSetting(spotymc_session_remember)</usealttexture>
		            <onclick>Skin.ToggleSetting(spotymc_session_remember)</onclick>
		        </control>
		        
		        <control type="button" id="1104">
		        	<include>spotymc_login_button</include>
			        <description>Login button</description>
			        <label>Login</label>
			        <posx>32</posx>
			        <posy>224</posy>
			        <onclick></onclick>
			        <onfocus></onfocus>
			        <onup>1103</onup>
			        <ondown>1105</ondown>
			        <onleft>-</onleft>
			        <onright>1105</onright>
		        </control>
		        
		        <control type="button" id="1105">
		        	<include>spotymc_login_button</include>
			        <description>Cancel button</description>
			        <label>Cancel</label>
			        <posx>224</posx>
			        <posy>224</posy>
			        <onclick></onclick>
			        <onfocus></onfocus>
			        <onup>1103</onup>
			        <ondown>-</ondown>
			        <onleft>1104</onleft>
			        <onright>-</onright>
		        </control>
			</control>
        </control>
    </controls>
</window>
