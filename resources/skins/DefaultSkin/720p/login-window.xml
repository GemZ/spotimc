<window id="4002" type="dialog">
    <defaultcontrol always="false">1</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <backgroundcolor>0000000</backgroundcolor>
    <!-- <views>50,51,509,510<views> -->
    <!-- <animation effect="fade" time="100">WindowOpen</animation> -->
    <!-- <animation effect="slide" end="0,576" time="100">WindowClose</animation> -->
    
    
    <!-- Open animation -->
    <!--
    <animation type="WindowOpen">
    	<effect type="slide" center="640,360" time="150" start="0,-360" end="0,0" />
    </animation>
    
    <animation effect="fade" time="1000">WindowClose</animation>
    -->
    
    <visible>true</visible>
    <zorder>1</zorder>
    <coordinates>
	    <posx></posx>
	    <posy></posy>
    </coordinates>
    <previouswindow></previouswindow>
    <useincludes>false</useincludes>
    
    <controls>
    	<!-- Window background -->
    	<!--
    	<control type="image">
            <description>Background overlay</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <visible>true</visible>
            <colordiffuse>80ffffff</colordiffuse>
            <fadetime></fadetime>
            <texture>common/overlay.png</texture>
            <aspectratio>stretch</aspectratio>
        </control>
        -->
        
        <control type="group" id="1">
            <description>Login container</description>
            <defaultcontrol>2</defaultcontrol>
            <visible>true</visible>
            <posx>384</posx>
            <posy>160</posy>
            <width>480</width>
            <height>384</height>
            
            <animation type="WindowOpen">
		    	<effect type="slide" tween="cubic" easing="out" center="640,360" time="200" start="0,-600" end="0,0" />
		    </animation>
		    
		    <animation type="Hidden">
		    	<effect type="slide" tween="cubic" easing="in" center="640,360" time="200" start="0,0" end="0,600" />
		    </animation>
		    
		    <control type="image">
		    	<include>spotymc_dialog_border</include>
	            <description>Window border</description>
	            <posx>-16</posx>
	            <posy>-16</posy>
	            <width>512</width>
	            <height>416</height>
	        </control>
	        
	        <control type="image">
	            <description>Window background image</description>
	            <posx>0</posx>
	            <posy>0</posy>
	            <width>480</width>
	            <height>384</height>
	            <visible>true</visible>
	            <colordiffuse>ffffffff</colordiffuse>
	            <fadetime></fadetime>
	            <texture>common/login-bg.png</texture>
	            <aspectratio>stretch</aspectratio>
	        </control>
            
            <!-- Login prompt text -->
            <control type="label">
            	<label>Login to Spotify</label>
				<posx>32</posx>
				<posy>32</posy>
				<width>416</width>
				<height>32</height>
				<font>spotymc_font24</font>
				<align>center</align>
				<aligny>center</aligny>
				<selectedcolor></selectedcolor>
				<textcolor>ffffffff</textcolor>
			</control>
			
			<control type="label">
				<include>spotymc_login_inputlabel</include>
                <label>Username</label>
                <posx>32</posx>
                <posy>80</posy>
            </control>
            
			<control type="button" id="10">
				<include>spotymc_login_textinput</include>
				<description>Username input</description>
				<posx>32</posx>
				<posy>112</posy>
				<onclick></onclick>
				<onfocus></onfocus>
				<onup></onup>
				<ondown>11</ondown>
				<onleft></onleft>
				<onright></onright>
            </control>
            
            
            <control type="label">
            	<include>spotymc_login_inputlabel</include>
                <label>Password</label>
                <posx>32</posx>
                <posy>160</posy>
            </control>
            
            <control type="button" id="11">
            	<include>spotymc_login_textinput</include>
                <description>Password input</description>
                <posx>32</posx>
                <posy>192</posy>
                <label>******</label>
                <onclick></onclick>
                <onfocus></onfocus>
                <onup>10</onup>
                <ondown>12</ondown>
                <onleft></onleft>
                <onright></onright>
            </control>
            
            
            <control type="label">
            	<include>spotymc_login_inputlabel</include>
                <label>Remember me</label>
                <posx>72</posx>
                <posy>248</posy>
            </control>
            
            <control type="togglebutton" id="12">
            	<include>spotymc_login_toggle</include>
	            <description>Remember me toggle</description>
	            <posx>32</posx>
	            <posy>248</posy>
	            <onfocus></onfocus>
	            <onup>11</onup>
	            <ondown>13</ondown>
	            <onleft></onleft>
	            <onright></onright>
	            <usealttexture>Skin.HasSetting(spotymc_session_remember)</usealttexture>
	            <onclick>Skin.ToggleSetting(spotymc_session_remember)</onclick>
	        </control>
	        
	        <control type="button" id="13">
	        	<include>spotymc_login_button</include>
		        <description>Login button</description>
		        <label>Login</label>
		        <posx>64</posx>
		        <posy>304</posy>
		        <onclick></onclick>
		        <onfocus></onfocus>
		        <onup>12</onup>
		        <ondown>14</ondown>
		        <onleft>-</onleft>
		        <onright>14</onright>
	        </control>
	        
	        <control type="button" id="14">
	        	<include>spotymc_login_button</include>
		        <description>Cancel button</description>
		        <label>Cancel</label>
		        <posx>256</posx>
		        <posy>304</posy>
		        <onclick></onclick>
		        <onfocus></onfocus>
		        <onup>12</onup>
		        <ondown>-</ondown>
		        <onleft>13</onleft>
		        <onright>-</onright>
	        </control>
	        
        </control>
    </controls>
</window>
